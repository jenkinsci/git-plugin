<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:entry title="Name" field="name">
        <f:textbox name="parameter.name" value="${instance.name}" />
    </f:entry>
    <f:entry title="Repository URL" field="repo">
        <f:textbox name="parameter.repo" value="${instance.repo}" />
    </f:entry>
    <f:entry title="List references in reverse order" field="reverse">
        <f:checkbox name="parameter.reverse" checked="${instance.reverse}" />
    </f:entry>
</j:jelly>
